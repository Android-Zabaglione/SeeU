<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.seeu">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/small_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/small_logo_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SubActivity"></activity>
        <activity android:name=".TutorialActivity" />
        <activity android:name=".ConcertInfoActivity" />
        <activity android:name=".ReadReviewActivity" />
        <activity android:name=".WriteReviewActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".ConcertActivity" />
        <activity
            android:name=".ui.login.LoginActivity"
            android:label="@string/title_activity_login" />
        <activity android:name=".StartingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MyReviewActivity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>